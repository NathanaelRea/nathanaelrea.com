---
import Layout from "../layouts/Layout.astro";
import ImageContainer from "../layouts/ImageContainer.astro";
import OptimizedImage from "../components/OptimizedImage.astro";
import i3 from "../assets/Photos/2024-01-19.1.jpg";
import i4 from "../assets/Photos/2024-01-19.2.jpg";
import i6 from "../assets/Photos/2024-01-20.2.jpg";
import i7 from "../assets/Photos/2024-07-28.jpg";
import i8 from "../assets/Photos/2024-08-30.jpg";
---

<Layout title="Photos">
  <ImageContainer>
    <OptimizedImage src={i8} alt="2024-08-30" />
    <OptimizedImage src={i7} alt="2024-07-28" />
    <OptimizedImage src={i6} alt="2024-01-20.2" />
    <OptimizedImage src={i4} alt="2024-01-19.2" />
    <OptimizedImage src={i3} alt="2024-01-19.1" />
